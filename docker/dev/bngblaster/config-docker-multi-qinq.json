{
  "interfaces": {
    "access": [
      {
        "interface": "eth0",
        "type": "ipoe",
	      "vlan-mode": "1:1",
        "outer-vlan-min": 100,
        "outer-vlan-max": 110,
        "inner-vlan-min": 2,
        "inner-vlan-max": 4092
      }
    ]
  },
  "dhcp": {
    "enable": true,
    "retry": 25
  },
  "dhcpv6": {                                                                                                                                                         
    "enable": true,
    "ia-na": true,
    "ia-pd": true,
    "rapid-commit": false,
    "access-line": true,
    "ldra": true                                                                                                                                          
  }, 
  "ipoe": {
    "ipv6": true
  },
  "sessions": {
    "count": 1000,
    "max-outstanding": 400,
    "start-rate": 200,
    "stop-rate": 800
  },
  "access-line": {
    "agent-circuit-id": "eth 0/1/2/3/{session-global}:100.{session-global}",
    "agent-remote-id": "DSLAM-{session-global}"
  }
}
