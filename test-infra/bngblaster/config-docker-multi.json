{
  "interfaces": {
    "access": [
      {
        "interface": "bng-access",
        "type": "ipoe",
	      "vlan-mode": "N:1",
        "outer-vlan-min": 100,
        "outer-vlan-max": 100
      }
    ]
  },
  "sessions": {
    "count": 1,
    "autostart": true,
    "start-rate": 100,
    "stop-rate": 200,
    "max-outstanding": 500
  },
  "dhcp": {
    "enable": true
  },
  "dhcpv6": {
    "enable": false
  },
  "ipoe": {
    "ipv6": false
  },
  "access-line": {
    "agent-circuit-id": "eth 0/1/2/3/{session-global}:100.{session-global}",
    "agent-remote-id": "DSLAM-{session-global}"
  },
  "session-traffic": {
    "ipv4-pps": 100
  }
}
